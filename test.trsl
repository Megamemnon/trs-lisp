(define (pass X) 
        (begin (display X) 
               (ansi 32) 
               (display " PASS") 
               (ansi-reset)
               (newline)))
(define (fail X) 
        (begin (display X) 
               (ansi 31) 
               (display " FAIL") 
               (ansi-reset)
               (newline)))

(if (= (+ 1 1) 2) 
    (pass "+") 
    (fail "+"))
(if (= (- 2 1) 1) 
    (pass "-") 
    (fail "-"))
(if (= (* 3 2) 6)
    (pass "*")
    (fail "*"))
(if (= (/ 6 2) 3)
    (pass "/")
    (fail "/"))
(if (= 1 1)
    (pass "=")
    (fail "="))
(if (< 2 3)
    (pass "<")
    (fail "<"))
(if (> 6 3)
    (pass ">")
    (fail ">"))
(if (<= 2 3)
    (pass "<=")
    (fail "<="))
(if (>= 6 3)
    (pass ">=")
    (fail ">="))

