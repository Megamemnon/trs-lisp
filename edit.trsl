(comment trs-lisp editor)
(load "libedit.trsl")
(init-editor)
(do ((i 0 0))
    ((> i 1) (display "done"))
    (begin  (set! ch (read-char))
            (cond   ((string=? "X" ch) (exit))
                    ((backspace? ch) (backspace))
                    (else   (display ch)
                    ))))
